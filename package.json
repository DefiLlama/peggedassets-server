    "node-html-parser": "^3.1.1",
    "pact-lang-api": "^4.3.5",
    "papaparse": "^5.3.0",
    "tronweb": "^3.2.7",
    "underscore": "^1.13.1",
    "web3": "^1.3.6"
  }
}
